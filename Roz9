zad1 
#include <stdio.h>
#include <stdlib.h>
float min(double x, double y);
int main()
{
    printf("%f", min(7,5));
    return 0;
}
float min(double x, double y){
    if(x<y){
        return x;
    }
    else if(y<x){
        return y;
    }
}

zad2
#include <stdio.h>
#include <stdlib.h>
void rzad(char ch, int i, int j);
int main()
{
    rzad('x', 3, 2);
    return 0;
}
void rzad(char ch, int a, int x)
{
    for(int i = 0; i<a; i++)
    {
        for(int j = 0; j<x; j++)
            {
            printf("%c", ch);
            }
        printf("\n");
    }
}



2.2.5
#include <stdio.h>
#include <math.h>
int main()
{
 float i;
 printf("Podaj potege: ");
 scanf("%f", &i);
 printf("2 do %.2f, to %.2f", i, pow(2, i));
 return 0;
}

2.2.6
#include <stdio.h>
#include <math.h>
int main()
{
 int x, y;
 printf("Podaj podstawe potegi: ");
 scanf("%d", &x);
 printf("Podaj potege: ");
 scanf("%d", &y);
 printf("Potega wynosi %.2f", pow(x, y));
 return 0;
}


2.2.33

#include <stdio.h>
#include <stdlib.h>
void def(unsigned int a, unsigned int b, unsigned int c, unsigned int d,unsigned int e);
int main()
{
int a,b,c,d,e,result;
printf("a: \n");
scanf("%d", &a);
printf("b: \n");
scanf("%d", &b);
printf("c: \n");
scanf("%d", &c);
printf("d: \n");
scanf("%d", &d);
printf("e: \n");
scanf("%d", &e);

printf("Wynik a+b= %d\n", a+b);
printf("Wynik a+b+c= %d\n", a+b+c);
printf("Wynik a+b+c+d= %d\n", a+b+c+d);
printf("Wynik a+b+c+d+e= %d\n", a+b+c+d+e);
}
